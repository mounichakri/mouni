<script>
    var jVarData = {{{ data }}}

    //  document.getElementById("ldlID").innerHTML = jVarData[0].username;

    $(document).ready(function () {
        $('#tableID').DataTable({
            "aLengthMenu": [[2, 4, 8, -1], [2, 4, 8, "All"]],
            "iDisplayLength": 2,
            "order": [[0, "desc"]]
        });
    });

    jFTable();
    function jFTable(inpage) {

        let tableid = document.getElementById("tableID");
        let thead = document.createElement('thead');
        let tbody = document.createElement('tbody');

        let tdr;
        let td;
        let th;
        let tr;
        jVarData.forEach((i, j) => {
            tr = document.createElement('tr');
            ////// columns //////////
            Object.keys(i).forEach(key => {
                th = document.createElement('th');
                th.innerHTML = key;
                tr.appendChild(th);
            });

            ////////// row //////
            tdr = document.createElement('tr');
            Object.values(i).forEach(key => {
                td = document.createElement('td');
                td.innerHTML = key;
                tdr.appendChild(td);
            });
            tbody.appendChild(tdr);
        })



        console.log(tbody);
        thead.appendChild(tr);

        tableid.appendChild(thead);
        tableid.appendChild(tbody);
    }

    function jFLogout() {
        window.location.href = "/";
    }

</script>

